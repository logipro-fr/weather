{
    "name": "logipro/weather",
    "type": "app",
    "description": "Weather sharing",
    "keywords": ["weather"],
    "homepage": "https://github.com/logipro-fr/weather",
    "license": "Copyrighted",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Frédéric Royet",
            "email": "frederic.royet@logipro.com",
            "homepage": "https://www.linkedin.com/in/frederic-royet-42568566/",
            "role": "Project Founder"
        }
    ],
    "require": {
        "php": ">=8.3",
        "phariscope/safephp": "^1.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/http-kernel": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "doctrine/orm": "^2.17",
        "symfony/runtime": "^7.0",
        "doctrine/doctrine-bundle": "^2.11",
        "symfony/orm-pack": "^2.4"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.27",
        "phpstan/phpdoc-parser": "^1.20",
        "phpstan/phpstan": "1.10.15",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "3.*",
        "behat/behat": "^3.13",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4" : {
            "Weather\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Weather\\Tests\\" : "tests/unit",
            "Weather\\Integration\\": "tests/integration"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "symfony/runtime": true
        }
    }
}
